-- creere tabela pentru masini
CREATE TABLE cars (
    id VARCHAR(36) PRIMARY KEY,
    brand VARCHAR(50),
    model VARCHAR(50),
    year INT,
    km INT,
    price DECIMAL(10,2),
    type VARCHAR(30),
    mainImage VARCHAR(255),
    additionalImages JSON,
    description TEXT,
    engineSize INT,
    horsepower INT,
    fuelType VARCHAR(20),
    gearbox VARCHAR(10)
);

